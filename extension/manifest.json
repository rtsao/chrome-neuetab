{
  "name": "NeueTab",
  "version": "0.0.0",
  "manifest_version": 2,
  "description": "A new tab page replacement for Google Chrome",
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "override_newtab/index.html"
  },
  "web_accessible_resources": [
    "override_newtab/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "http://www.google.com/",
    "http://api.openweathermap.org/"
  ]
}