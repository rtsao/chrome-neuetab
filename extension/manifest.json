{
  "name": "NeueTab",
  "version": "0.0.0",
  "manifest_version": 2,
  "description": "A new tab page replacement for Google Chrome",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "override_newtab/index.html"
  },
  "web_accessible_resources": [
    "override_newtab/*"
  ]
}